<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="5dip" />
            <stroke android:width="1dip" android:color="#101C5A" />
            <gradient android:angle="-90" android:startColor="#0A21A1" android:endColor="#1C2D8A"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="5dip" />
            <stroke android:width="1dip" android:color="#091868" />
            <solid android:color="#3F51B5"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="5dip" />
            <stroke android:width="1dip" android:color="#091868" />
            <gradient android:angle="-90" android:startColor="#4763FF" android:endColor="#4763FF" />
        </shape>
    </item>
</selector>